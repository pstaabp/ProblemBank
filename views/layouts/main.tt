<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="[% settings.charset %]">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=yes">

  <title>problemdb</title>

  <link rel="stylesheet" type="text/css" href="[% request.uri_base %]/javascripts/bower_components/bootstrap/dist/css/bootstrap.min.css">
  <link rel="stylesheet" type="text/css" href="[% request.uri_base %]/javascripts/bower_components/font-awesome/css/font-awesome.min.css">
  <link rel="stylesheet" type="text/css"  href="[% request.uri_base %]/javascripts/bower_components/bootstrap-markdown/css/bootstrap-markdown.min.css">
  <link rel="stylesheet" type="text/css" href="[% request.uri_base %]/css/style.css">

 <script type="text/javascript" src="[% request.uri_base %]/javascripts/apps/require-config.js"></script>


<script type="text/javascript" data-main="[% request.uri_base %]/javascripts/apps/[% appname %]" src="/javascripts/bower_components/requirejs/require.js"></script>

<script type="text/javascript">
requirejs.config({
    config: {
        '[% appname %]': {
            [% IF authors %]
              authors: [% authors %],
            [% END %]
            [% IF modules %]
              modules: [% modules %],
            [% END %]
            [% IF problems %]
              problems: [% problems %],
            [% END %]
            [% IF problem_sets %]
              problem_sets: [% problem_sets %],
            [% END %]
            [% IF id %]
               problem_id: "[% id %]",
            [% END %]
            [% IF set_id %]
               set_id: "[% set_id %]",
            [% END %]

        }
    }
});
</script>

</head>
<body>
    <!-- Static navbar -->
    <nav class="navbar navbar-default navbar-static-top">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="#">ProblemBank</a>
        </div>
        <div id="navbar" class="navbar-collapse collapse">
          <ul class="nav navbar-nav">
            <li class="active"><a href="/">Home</a></li>
            <li><a href="/problems">Problems</a></li>
            <li><a href="/problemsets">Problem Sets</a></li>
            <li class="dropdown">
              <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Other<span class="caret"></span></a>
              <ul class="dropdown-menu">
                <li><a href="/modules">Edit Modules</a></li>
                <li><a href="/authors">Edit Authors</a></li>
                <li><a href="/problem">Add a problem</a></li>
                <li role="separator" class="divider"></li>
                <li class="dropdown-header">Nav header</li>
                <li><a href="#">Separated link</a></li>
                <li><a href="#">One more separated link</a></li>
              </ul>
            </li>
          </ul>
          <div class="nav navbar-nav navbar-right">
            <p id="message">Message</p>
          </div>

        </div><!--/.nav-collapse -->
      </div>
    </nav>
  <div class="container">
  <div id="page" class="row">
    <div id="content" class="col-sm-9">

    </div>
    <div class="col-sm-3">
    [% INCLUDE sidebar.tt %]
    </div>
  </div>
  </div>
  <div id="footer">
    Powered by <a href="http://perldancer.org/">Dancer2</a> [% dancer_version %]
  </div>
  [% content %]
</body>
</html>
